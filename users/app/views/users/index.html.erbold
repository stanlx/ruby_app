<h1>Listing users</h1>
    <table data-source= "<%= users_url(format: "json") %" id="users" class="display table table-bordered table-hover table-condensed">
      <thead>
      <tr>
        <th>Fname</th>
        <th>Lname</th>
        <th>Email</th>
      </tr>
      </thead>
      <tbody>
      <% @users.each do |user| %>
      <tr>
        <td><%= user.first_name %></td>
        <td>Last_name</td>
        <td>email</td>
      </tr>
      <% end %>
      <tbody>
    </table>
<br />

